{
	"frameBlocks": [
		{
			"registryName": "minecraft:sponge",
			//32767 is the wildcard value
			"meta": 32767,
			"minimumAmount": 1
		},
		{
			"registryName": "minecraft:diamond_block",
			"meta": 32767,
			"minimumAmount": 2
		},
		{
			//To specify ore dictionary names, use the format ore:<name>
			"registryName": "ore:stairWood",
			//This has no effect since it's an ore dictionary name
			"meta": 32767,
			"minimumAmount": 1
		}
	],
	//Valid values: "ANY", "ANY_NON_AIR", "SAME" (same as frameBlocks)
	"requiredCorner": "ANY_NON_AIR",
	//If true, corner blocks will also contribute to minimumAmount
	"cornerBlocksContributeToMinimumAmount": true,
	//Valid values: "LATERAL", "VERTICAL", "VERTICAL_X", "VERTICAL_Z", "LATERAL_OR_VERTICAL"
	"type": "LATERAL_OR_VERTICAL",
	//Whether generated portal frames drop (set to false to prevent frame block farming)
	//This should also work with other mods such as Block Breakers from OpenBlocks and Exchangers
	"doGeneratedFramesDrop": true,
	"activation": {
		//Whether the portal frame is activated when fire is placed inside it
		"canBeActivatedByFire": true,
		//Items that can be used to activate the Nether portal
		"activators": [
			{
				"registryName": "minecraft:diamond_block",
				"meta": 32767
			}
		],
		//What happens when one of the above activators is used. Valid values:
		//"CONSUME": shrink the activator stack by 1
		//"DAMAGE": damage the activator stack by 1
		//"DO_NOTHING": do nothing to the activator stack
		"activatorConsumeBehavior": "CONSUME",
		//Whether fire should be spawned for a tick before the portal is activated
		"spawnFireBeforeActivating": true,
		//When one of the above activators is used, a random sound is selected from here
		//with a random pitch
		"activationSounds": [
			"minecraft:entity.enderdragon.death"
		]
	},
	//Valid values: "WHITE", "ORANGE", "MAGENTA", "LIGHT_BLUE", "YELLOW", "LIME", "PINK", "GRAY",
	//"SILVER", "CYAN", "PURPLE", "BLUE", "BROWN", "GREEN", "RED", "BLACK"
	//When the portal is activated, a random color is selected
	"colors": [
		"ORANGE",
		"LIME"
	],
	//If true, the portal cannot be dyed to a color not defined above
	"forceColor": true,
	//If true, "dimensions" is a whitelist rather than a blacklist
	"whitelist": false,
	//Whitelist or blacklist of dimensions this portal type can be activated in
	"dimensions": [],
	//The destination dimension ID
	"dimensionID": 1,
	//Whether to spawn a receiving portal (only works if teleportToPortal is true)
	"spawnPortal": true,
	//Whether to teleport to a receiving portal
	"teleportToPortal": true,
	"size": {
		"lateral": {
			"minWidth": 3,
			"maxWidth": 9000,
			"minHeight": 3,
			"maxHeight": 9000
		},
		"verticalX": {
			"minWidth": 3,
			"maxWidth": 9000,
			"minHeight": 3,
			"maxHeight": 9000
		},
		"verticalZ": {
			"minWidth": 3,
			"maxWidth": 9000,
			"minHeight": 3,
			"maxHeight": 9000
		}
	},
	"teleportationDelay": {
		//How long an entity has to stay in a portal to get teleported
		"lateral": 1,
		//If 0, the entity default is used
		"verticalX": 0,
		"verticalZ": 0
	}
}
