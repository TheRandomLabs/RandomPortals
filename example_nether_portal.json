//Please note that this format will most likely change before the first release.
{
	"frameBlocks": [
		{
			"registryName": "minecraft:sponge",
			//32767 is the wildcard value
			"meta": 32767,
			"minimumAmount": 1
		},
		{
			"registryName": "minecraft:diamond_block",
			"meta": 32767,
			"minimumAmount": 2
		},
		{
			//To specify ore dictionary names, use the format ore:<name>
			"registryName": "ore:stairWood",
			//This has no effect since it's an ore dictionary name
			"meta": 32767,
			"minimumAmount": 1
		},
		{
			"registryName": "minecraft:sponge",
			"meta": 32767,
			"minimumAmount": 0
		}
	],
	//Valid values: "ANY", "ANY_NON_AIR", "SAME" (same as frameBlocks)
	"requiredCorner": "ANY_NON_AIR",
	//If true, corner blocks will also contribute to minimumAmount
	"cornerBlocksContributeToMinimumAmount": false,
	//Whether generated portal frames should drop
	//Set this to false to prevent frame block farming
	"doGeneratedFramesDrop": true,
	//If true, "dimensions" is a whitelist rather than a blacklist
	"whitelist": false,
	//Whitelist or blacklist of dimensions this portal type can be activated in
	"dimensions": [],
	//The destination dimension ID
	"dimensionID": 1,
	//If the destination dimension is the End (ID 1), whether to force spawn a portal rather than an obsidian platform
	"forcePortal": true,
	//The values in verticalendportals/nether_portal are checked first, and these are checked second
	"minWidth": 4,
	"maxWidth": 4,
	"minHeight": 5,
	"maxHeight": 5
}
